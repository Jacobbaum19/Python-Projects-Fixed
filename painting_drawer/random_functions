import random
color_list = [(209, 163, 92), (241, 222, 232), (162, 75, 55), (230, 204, 102), (69, 114, 151), (229, 242, 234), (225, 233, 243), (60, 98, 66), (176, 166, 48), (160, 59, 69), (199, 161, 179), (219, 175, 196), (36, 36, 69), (129, 157, 182), (201, 94, 70), (51, 50, 95), (40, 81, 45), (147, 169, 152), (33, 65, 37), (84, 128, 179), (183, 86, 95), (224, 177, 168), (244, 201, 4), (52, 34, 56), (132, 39, 52), (133, 40, 32), (66, 76, 35), (54, 52, 29), (182, 187, 212)]


def random_color():
    return random.choice(color_list)


def rgb_to_hex(rgb):
    return "#{:02x}{:02x}{:02x}".format(rgb[0], rgb[1], rgb[2])


def random_spot_choice():
    return random.choice([True,False])


